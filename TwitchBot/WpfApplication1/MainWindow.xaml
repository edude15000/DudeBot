                                    </StackPanel>

                                    <GroupBox  Header="Currency Settings">
                                        <StackPanel>
                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="Currency Name" />
                                            <TextBox x:Name="currencyName" FontSize="14" Text="{Binding currency.currencyName, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="Command Command" />
                                            <TextBox x:Name="currencyCommand" FontSize="14" Text="{Binding currency.currencyCommand, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="Payout per minute (Viewers)" />
                                            <Controls:NumericUpDown x:Name="currencyPerMinute" Minimum = "0" Maximum = "100" Interval = "1" HorizontalAlignment="Left" Value="{Binding currency.currencyPerMinute, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="Payout per minute (Subs)" />
