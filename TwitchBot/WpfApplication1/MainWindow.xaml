                                            <StackPanel Margin="0 10 0 0" Orientation="Horizontal">
                                                <TextBlock VerticalAlignment="Center" FontSize="14" Text="Gamble on / off " />
                                                <CheckBox x:Name="gambleToggle" VerticalAlignment="Center" FontSize="14" Checked="gamble_changed" Unchecked="gamble_changed" IsChecked="{Binding currency.gambleToggle, UpdateSourceTrigger=PropertyChanged}"/>
                                            </StackPanel>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="Max Gamble" />
                                            <Controls:NumericUpDown x:Name="maxGamble" Minimum = "1" Maximum = "1000000" Interval = "1" HorizontalAlignment="Left" Value="{Binding currency.maxGamble, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="Gamble Cool Down (minutes)" />
                                            <Controls:NumericUpDown x:Name="gambleCoolDown" Minimum = "1" Maximum = "1000" Interval = "1" HorizontalAlignment="Left" Value="{Binding currency.gambleCoolDownMinutes, UpdateSourceTrigger=PropertyChanged}"/>
                                        </StackPanel>
                                    </GroupBox>

                                    <GroupBox  Header="Standard Request Settings">
                                        <StackPanel>
                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="Standard requests COST" />
                                            <Controls:NumericUpDown x:Name="regSongCost" Minimum = "0" Maximum = "100000" Interval = "1" HorizontalAlignment="Left" Value="{Binding requestSystem.regSongCost, UpdateSourceTrigger=PropertyChanged}"/>

                                        </StackPanel>
                                    </GroupBox>

                                    <GroupBox  Header="VIP Song Settings">
                                        <StackPanel>

                                            <StackPanel Margin="0 10 0 0" Orientation="Horizontal">
                                                <TextBlock HorizontalAlignment="Right" FontSize="14" Text="VIP Songs on / off " />
                                                <CheckBox x:Name="vipsongtoggle" FontSize="14" Checked="vipsongcheck" Unchecked="vipsongcheck" IsChecked="{Binding currency.vipSongToggle, UpdateSourceTrigger=PropertyChanged}"/>
                                            </StackPanel>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="!vipsong COST" />
                                            <Controls:NumericUpDown x:Name="vipsongcost" Minimum = "1" Maximum = "100000" Interval = "1" HorizontalAlignment="Left" Value="{Binding currency.vipSongCost, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="VIP Song Redeem Cool Down Per User (minutes)" />
                                            <Controls:NumericUpDown x:Name="vipRedeemCoolDownMinutes" Minimum = "0" Maximum = "1000" Interval = "1" HorizontalAlignment="Left" Value="{Binding currency.vipRedeemCoolDownMinutes, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="VIP Song Redeem Cool Down Overall (minutes)" />
                                            <Controls:NumericUpDown x:Name="vipRedeemCoolDownMinutesOverall" Minimum = "0" Maximum = "1000" Interval = "1" HorizontalAlignment="Left" Value="{Binding currency.vipRedeemCoolDownMinutesOverall, UpdateSourceTrigger=PropertyChanged}"/>
                                        </StackPanel>
                                    </GroupBox>

                                    <GroupBox  Header="Sub Credit Settings">
                                        <StackPanel>

                                            <TextBlock Padding="0 10 0 0" FontSize="14" Text="Sub Credit Song Redeem Cost" />
                                            <Controls:NumericUpDown x:Name="subCreditRedeemCost" Minimum = "0" Maximum = "100" Interval = "1" HorizontalAlignment="Left" Value="{Binding currency.subCreditRedeemCost, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBlock Padding="0 10 0 0" VerticalAlignment="Center" FontSize="14" Text="Sub Credit Rewarded Per Sub" />
                                            <Controls:NumericUpDown x:Name="creditsPerSub" Minimum = "0" Maximum = "100" Interval = "1" HorizontalAlignment="Left" Value="{Binding currency.creditsPerSub, UpdateSourceTrigger=PropertyChanged}"/>
                                        </StackPanel>
                                    </GroupBox>
                                    
                                    <Button Margin="0 10 0 0" Style="{DynamicResource SquareButtonStyle}" x:Name="GO2" Content="Apply" Click="writeToConfig" />

                                </StackPanel>
                            </Border>
                        </ScrollViewer>
