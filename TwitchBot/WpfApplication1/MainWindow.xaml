                                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                                <TextBlock VerticalAlignment="Center" FontSize="14" Text="Auto promote subscriber's next request to VIP " />
                                                <CheckBox x:Name="autoPromoteToVipOnSub" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding requestSystem.autoPromoteToVipOnSub, UpdateSourceTrigger=PropertyChanged}"/>
                                            </StackPanel>

                                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                                <TextBlock VerticalAlignment="Center" FontSize="14" Text="Auto promote subscriber's next request to $$$ " />
                                                <CheckBox x:Name="autoPromoteToDonatorOnSub" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding requestSystem.autoPromoteToDonatorOnSub, UpdateSourceTrigger=PropertyChanged}"/>
                                            </StackPanel>

                                        </StackPanel>
                                    </GroupBox>
                                    <GroupBox Header="Bit Donation Events">
                                        <StackPanel>
                                            <TextBlock FontSize="14" Text="Bit Donation Message" />
                                            <TextBox Margin="0 0 0 10" TextWrapping="Wrap" x:Name="bitMessage" FontSize="14" Text="{Binding bitMessage, UpdateSourceTrigger=PropertyChanged}" />

                                            <TextBlock FontSize="14" Text="Bit Donation Command" />
                                            <TextBox Margin="0 0 0 10" TextWrapping="Wrap" x:Name="bitMessageCommand" FontSize="14" Text="{Binding bitMessageCommand, UpdateSourceTrigger=PropertyChanged}" />

                                            <TextBlock FontSize="14" Text="Minimum bit donation required for auto VIP promotion" />
                                            <Controls:NumericUpDown Margin="0 0 0 10" x:Name="autoPromoteToVipOnBitDonationMinimum" Minimum = "1" Maximum = "10000" Interval = "1" HorizontalAlignment="Left" Value="{Binding requestSystem.autoPromoteToVipOnBitDonationMinimum, UpdateSourceTrigger=PropertyChanged}"/>

                                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                                <TextBlock VerticalAlignment="Center" FontSize="14" Text="Auto promote donator's next request to VIP " />
                                                <CheckBox x:Name="autoPromoteToVipOnBitDonation" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding requestSystem.autoPromoteToVipOnBitDonation, UpdateSourceTrigger=PropertyChanged}"/>
                                            </StackPanel>

                                            <TextBlock FontSize="14" Text="Minimum bit donation required for auto $$$ promotion" />
                                            <Controls:NumericUpDown Margin="0 0 0 10" x:Name="autoPromoteToDonatorOnBitDonationMinimum" Minimum = "1" Maximum = "10000" Interval = "1" HorizontalAlignment="Left" Value="{Binding requestSystem.autoPromoteToDonatorOnBitDonationMinimum, UpdateSourceTrigger=PropertyChanged}"/>

                                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                                <TextBlock VerticalAlignment="Center" FontSize="14" Text="Auto promote donator's next request to $$$ " />
                                                <CheckBox x:Name="autoPromoteToDonatorOnBitDonation" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding requestSystem.autoPromoteToDonatorOnBitDonation, UpdateSourceTrigger=PropertyChanged}"/>
                                            </StackPanel>

                                        </StackPanel>
                                    </GroupBox>
                                    <GroupBox Header="Host/Raid Events">
                                        <StackPanel>
                                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                                <TextBlock  VerticalAlignment="Center" FontSize="14" Text="Auto Shoutout on Host " />
                                                <CheckBox x:Name="autoShoutoutOnHost" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding autoShoutoutOnHost, UpdateSourceTrigger=PropertyChanged}"/>
                                            </StackPanel>

                                            <TextBlock FontSize="14" Text="Host Message" />
                                            <TextBox Margin="0 0 0 10" TextWrapping="Wrap" x:Name="hostMessage" FontSize="14" Text="{Binding hostMessage, UpdateSourceTrigger=PropertyChanged}" />

                                            <TextBlock FontSize="14" Text="Host Command" />
                                            <TextBox Margin="0 0 0 10" TextWrapping="Wrap" x:Name="hostMessageCommand" FontSize="14" Text="{Binding hostMessageCommand, UpdateSourceTrigger=PropertyChanged}" />

                                            <TextBlock FontSize="14" Text="Host viewers required for raid" />
                                            <Controls:NumericUpDown Margin="0 0 0 10" x:Name="raidViewersRequired" Minimum = "1" Maximum = "100" Interval = "1" HorizontalAlignment="Left" Value="{Binding raidViewersRequired, UpdateSourceTrigger=PropertyChanged}"/>

                                            <TextBlock FontSize="14" Text="Raid Message" />
                                            <TextBox Margin="0 0 0 10" TextWrapping="Wrap" x:Name="raidMessage" FontSize="14" Text="{Binding raidMessage, UpdateSourceTrigger=PropertyChanged}" />

                                            <TextBlock FontSize="14" Text="Raid Command" />
                                            <TextBox Margin="0 0 0 10" TextWrapping="Wrap" x:Name="raidMessageCommand" FontSize="14" Text="{Binding raidMessageCommand, UpdateSourceTrigger=PropertyChanged}" />
                                        </StackPanel>
                                    </GroupBox>
                                    <Button FontSize="14" Style="{DynamicResource SquareButtonStyle}" x:Name="apply_button" Content="Apply" Click="writeToConfig" />
                                </StackPanel>
                            </Border>
                        </ScrollViewer>
                    </TabItem>
                </TabControl>
            </TabItem>

            <TabItem Header="Rocksmith" FontSize="14">
                <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" >
                    <Border Padding="10 0 10 0">
                        <StackPanel>
                            
                            <Button Margin="0 10 0 10" FontSize="14" Style="{DynamicResource SquareButtonStyle}" Content="Open RockSniffer" Click="openRockSniffer" />

                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock FontSize="14" Text="Open RockSniffer on startup " />
                                <CheckBox x:Name="openRockSnifferOnStartUp" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding openRockSnifferOnStartUp, UpdateSourceTrigger=PropertyChanged}"/>
                            </StackPanel>

                            <Button Margin="0 0 0 10" FontSize="14" Style="{DynamicResource SquareButtonStyle}" Content="Connect With CustomsForge" Click="customsForgeWindow" />

                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock FontSize="14" Text="Check CustomsForge for requests " />
                                <CheckBox x:Name="checkCustomsForge" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding requestSystem.checkCustomsForge, UpdateSourceTrigger=PropertyChanged}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock FontSize="14" Text="Open CustomsForge link on next song " />
                                <CheckBox x:Name="displayCFLink" FontSize="14" VerticalAlignment="Center" IsChecked="{Binding requestSystem.displayCFLink, UpdateSourceTrigger=PropertyChanged}"/>
                            </StackPanel>

                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock TextWrapping="Wrap" FontSize="14" Text="Open CustomsForge link instead of Youtube link by double clicking on song in queue " />
                                <CheckBox x:Name="openCFLinkInsteadOfYoutube" FontSize="14" VerticalAlignment="Center" IsChecked="{Binding requestSystem.openCFLinkInsteadOfYoutube, UpdateSourceTrigger=PropertyChanged}"/>
                            </StackPanel>

                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock FontSize="14" Text="Require Dynamic Difficulty " />
                                <CheckBox x:Name="requireDD" FontSize="14" VerticalAlignment="Center" IsChecked="{Binding requestSystem.requireDD, UpdateSourceTrigger=PropertyChanged}"/>
                            </StackPanel>

                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock HorizontalAlignment="Right" FontSize="14" Text="Require Lead Guitar " />
                                <CheckBox x:Name="checkCustomsForgeLead" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding requestSystem.checkCustomsForgeLead, UpdateSourceTrigger=PropertyChanged}"/>
                            </StackPanel>

                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock FontSize="14" Text="Require Rhythm Guitar " />
                                <CheckBox x:Name="checkCustomsForgeRhythm" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding requestSystem.checkCustomsForgeRhythm, UpdateSourceTrigger=PropertyChanged}"/>
                            </StackPanel>

                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock FontSize="14" Text="Require Bass Guitar " />
                                <CheckBox x:Name="checkCustomsForgeBass" VerticalAlignment="Center" FontSize="14" IsChecked="{Binding requestSystem.checkCustomsForgeBass, UpdateSourceTrigger=PropertyChanged}"/>
                            </StackPanel>

                            <StackPanel Margin="0 0 0 10" Orientation="Horizontal">
                                <TextBlock FontSize="14" Text="Tunings: Between E_STANDARD and " />
                                <ComboBox x:Name="lowestTuning" VerticalAlignment="Center" MaxDropDownHeight="200" SelectedIndex="{Binding requestSystem.lowestTuning, UpdateSourceTrigger=PropertyChanged}" >
                                    <ComboBoxItem>All Tunings</ComboBoxItem>
                                    <ComboBoxItem>E_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>DROP_D</ComboBoxItem>
                                    <ComboBoxItem>Eb_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>Eb_DROP_Db</ComboBoxItem>
                                    <ComboBoxItem>D_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>D_DROP_C</ComboBoxItem>
                                    <ComboBoxItem>Cs_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>Cs_DROP_B</ComboBoxItem>
                                    <ComboBoxItem>C_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>C_DROP_Bb</ComboBoxItem>
                                    <ComboBoxItem>B_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>B_DROP_A</ComboBoxItem>
                                    <ComboBoxItem>Bb_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>Bb_DROP_Ab</ComboBoxItem>
                                    <ComboBoxItem>A_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>Ab_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>G_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>LOW_Gb_STANDARD</ComboBoxItem>
                                    <ComboBoxItem>LOW_F_STANDARD</ComboBoxItem>
                                </ComboBox>
                            </StackPanel>

                            <Button Margin="0 0 0 10" FontSize="14" Style="{DynamicResource SquareButtonStyle}" Content="Apply" Click="writeToConfig" />
                      
                        </StackPanel>
                    </Border>
                </ScrollViewer>
            </TabItem>

            <TabItem Header="Notes" FontSize="14">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="90*"/>
                        <RowDefinition Height="8*"/>
                    </Grid.RowDefinitions>
                    <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" Grid.Row="0" >
                        <TextBox TextWrapping="Wrap" x:Name="notestextbox" FontSize="16" Text="" />
                    </ScrollViewer>
                    <Button FontSize="14" Style="{DynamicResource SquareButtonStyle}" Content="Save" Click="saveNotes" Grid.Column="0" Grid.Row="1" />
                </Grid>
            </TabItem>

            <TabItem Header="Readme" FontSize="14">
                <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" >
                    <TextBox IsReadOnly="True" TextWrapping="Wrap" x:Name="readmetextbox" FontSize="16" Text="" />
                </ScrollViewer>
            </TabItem>


        </TabControl>
    </Grid>
</Controls:MetroWindow>